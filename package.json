{
  "name": "ts-functools",
  "description": "functional for typescript",
  "version": "0.0.1",
  "scripts": {
    "compile": "tsc",
    "build": "rm -rf dist && tsc && npm run build_js",
    "list": "tsc --listFiles",
    "build_js": "tsc -p tsconfig_js.json && cp src/*.d.ts dist/",
    "test": "./node_modules/.bin/mocha --require ts-node/register ./test/*.spec.ts"
  },
  "devDependencies": {
    "ts-node": "^7.0.0",
    "mocha": "^5.2.0",
    "@types/mocha": "^5.2.4",
    "chai": "^4.1.2",
    "@types/chai": "^4.1.4",
    "prettier": "^1.13.7",
    "typescript": "^2.9.2"
  }
}
